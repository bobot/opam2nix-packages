  configurePhase = "true";
  installPhase = "${opam2nix}/bin/opam2nix invoke install";
  name = "syndic-1.3";
  opamEnv = builtins.toJSON 
  {
    deps = opamDeps;
    files = null;
    name = "syndic";
    ocaml-version = world.ocamlVersion;
    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "03an9cl5pdf9h56z7mg5c1q94jif45a777r3jgaahhq1di1fhrb8";
    url = "https://github.com/Cumulus/Syndic/releases/download/v1.3/syndic-1.3.tar.gz";
