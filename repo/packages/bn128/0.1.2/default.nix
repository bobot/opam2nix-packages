  {
    deps = opamDeps;
    files = null;
    name = "bn128";
    ocaml-version = world.ocamlVersion;
    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "0yc29ml8cx3njsszzxjnihcs0nwscnb29ygmbc3nzm9bainyjbw7";
    url = "https://github.com/runtimeverification/bn128-ml/archive/0.1.2.tar.gz";
