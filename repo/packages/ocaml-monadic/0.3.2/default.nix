    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "0vg819i181xfp7pbkcxhj0qy5ky17sfpdv5lapnddkj4k8vyi151";
