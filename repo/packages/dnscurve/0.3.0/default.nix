  installPhase = "${opam2nix}/bin/opam2nix invoke install";
  name = "dnscurve-0.3.0";
  opamEnv = builtins.toJSON 
  {
    deps = opamDeps;
    files = null;
    name = "dnscurve";
    ocaml-version = world.ocamlVersion;
    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "1ijdx6qwm478fqayf140hxvr5axyr5cmymi7qrsmnsa3db5j0fzn";
    url = "https://github.com/dsheets/ocaml-dnscurve/archive/0.3.0.tar.gz";
