  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "122ca4pyilw96azzgg7wgli2kqgzxiwsjqbv4ifl00i750dv1qb0";
