  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "1frm18disc398hq6v32s3yb8mm64c3l22njlr4i7d45hrr5z6ihk";
