  {
    deps = opamDeps;
    files = null;
    name = "omonad";
    ocaml-version = world.ocamlVersion;
    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "06icw8bda4wvz3hgxv5795r7j3p6l3wfifzdhlpr5qaap39p0g2x";
