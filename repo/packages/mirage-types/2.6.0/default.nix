  {
    deps = opamDeps;
    files = null;
    name = "mirage-types";
    ocaml-version = world.ocamlVersion;
    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "0br5vbsrv75kivrf6bz6ma6gq87fraznq4fhc2yx39dk77rh8szn";
