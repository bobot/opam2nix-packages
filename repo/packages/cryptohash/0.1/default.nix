  configurePhase = "true";
  installPhase = "${opam2nix}/bin/opam2nix invoke install";
  name = "cryptohash-0.1";
  opamEnv = builtins.toJSON 
  {
    deps = opamDeps;
    files = null;
    name = "cryptohash";
    ocaml-version = world.ocamlVersion;
    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "1cajsm5k5czh65dpbszssxy4l2qym60mrvfvz62n3xf7f8fk4chv";
    url = "https://github.com/fdopen/cryptohash/releases/download/0.1/cryptohash-0.1.tar.gz";
