  installPhase = "${opam2nix}/bin/opam2nix invoke install";
  name = "imap-1.1.1";
  opamEnv = builtins.toJSON 
  {
    deps = opamDeps;
    files = null;
    name = "imap";
    ocaml-version = world.ocamlVersion;
    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "025vyzasczbk8i5srm491qxrs550syl4bl3aw5cn2nnknjndag7h";
    url = "https://github.com/nojb/ocaml-imap/archive/1.1.1.tar.gz";
