  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "0n3fnnhwrlxz5nivsjsw7k598dfnz1dwhwix2h0rl1ky7d755km3";
