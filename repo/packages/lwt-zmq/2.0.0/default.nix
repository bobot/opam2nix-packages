    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "1436b36wv7h2ijiacs0nbvj4022y44acp5n11bgc06q2xn6pas90";
