  {
    deps = opamDeps;
    files = null;
    name = "crdt-ml";
    ocaml-version = world.ocamlVersion;
    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "0wsvw9yidl4xd3xz9pf1mws631sg0bgwxnxxm7k6akyd0v02xgn9";
