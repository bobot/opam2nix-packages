  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "1fibk3zs7dxl5w8k3vjl95hpndc1zfq3amhnpy063azb313ankym";
